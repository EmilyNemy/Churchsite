<div class="container">
	<div class="thumbnail col-xs-8">	
		<div id="calendar"></div>
	</div>
	<div class="thumbnail col-xs-4" style="length: 429;">
		<div id="carousel-for-events" class="carousel slide" data-ride="carousel">
		<!-- Indicators -->
		<ol class="carousel-indicators">
			<li data-target="#carousel-for-events" data-slide-to="0" class="active"></li>
			<li data-target="#carousel-for-events" data-slide-to="1" class=""></li>
			<!-- <li data-target="#carousel" data-slide-to="2" class=""></li> -->
		</ol>

		<!-- Wrapper for slides -->
		<div class="carousel-inner">
			<div class="item active">
				<img src="/images/images.jpeg" alt="Image">
				<div class="carousel-caption">
					<h3>Some of the events</h3>
					<p>Flowers</p>
				</div>
			</div>
			<div class="item">
				<img src="/images/flowers.jpeg" alt="Flowers">
				<div class="carousel-caption">
					<h3>At the beach</h3>
					<p>More flowers</p>
				</div>
			</div>
		</div>

		<!-- Controls -->
		<a class="left carousel-control" href="#carousel-for-events" data-slide="prev">
			<span class="glyphicon glyphicon-chevron-left"></span>
		</a>
		<a class="right carousel-control" href="#carousel-for-events" data-slide="next">
			<span class="glyphicon glyphicon-chevron-right"></span>
		</a>
	</div>
	</div>
</div>